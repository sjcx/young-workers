<div class="page-header">
	<h3>Interviews</h3>
</div>
<!-- header links -->
<%= render 'headerlinks' %>
<!-- body -->
<div class="container-fluid">
	<div class="row-fluid">
		<div class="span2">
			<% @contents.each do |content| %>
			<p>
				<a href="/contents/<%= content.id %>"><%= raw content.title  %></a> (<%= content.created_at.strftime('%B %e, %Y') %>)
			</p>

			<% end %>
		</div>
	</div>
</div>
